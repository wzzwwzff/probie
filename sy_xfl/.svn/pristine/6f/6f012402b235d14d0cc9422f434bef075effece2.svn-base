.content {
    background-color: #ffffff;
    color: inherit;
    padding: 10px 5px 10px 5px;
    border-color: #e7eaec;
    border-image: none;
    border-style: solid solid none;
    border-width: 1px 0;
}

.footer {
    text-align: center;
    position: fixed;
    width: 100%;
    bottom: 0px;
    left: 0px;
    right: 0px;
    color: inherit;
    border: 1px solid #e7eaec;
    background: #f5f5f6;
    padding: 8px 0 5px 0;
}

.search{
    min-height: 36px;
    line-height: 150%;
}





.count-info .label {
    line-height: 12px;
    padding: 2px 5px;
    position: absolute;
    right: 6px;
    top: 12px;
}

.logo-button{
    float:left;
    margin-top: 10px;
    margin-right: 10px;
}

.logo-title{
    font-size:25px;
    padding-top:10px;
    padding-left:6px;
    font-weight: bold;
    font-family: "微软雅黑";
}


/* TABLES */
