            line-height: 26px;
            font-size: 12px;
            overflow: hidden;
        }
        .myscroll ul{
            list-style: none;
        }
        .myscroll li {
            height: 40px;
            margin-left: 25px;
            font-size: 24px;
            color: white;
            font-weight: bold;
            line-height: 40px;
        }
        .myscroll a {
