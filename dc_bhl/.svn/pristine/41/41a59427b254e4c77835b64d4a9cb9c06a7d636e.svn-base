                    '10分指', '11分指', '12分指', '13分指', '14分指', "15分指", '16分指', '17分指', '18分指',
                ],
                title: {
                    text: ''
                },
                stackLabels: {
                    enabled: true,
                    style: {
                        fontWeight: 'bold',
                        color: '#000000'
                    },
                    useHTML: true,
                    formatter: function () {
                        return this.total;
                    }
                }
            },

            yAxis: {
                allowDecimals: false,
                min: 0,
                title: {
