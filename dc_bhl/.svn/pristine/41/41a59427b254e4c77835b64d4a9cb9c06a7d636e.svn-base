            },
            tooltip: {
                formatter: function () {
                    return '<b>' + this.x + '</b><br/>' +
                            this.series.name + ': ' + this.y + '<br/>' +
                            '共: ' + this.point.stackTotal + "户";
                }
            },
            plotOptions: {
                column: {
                    stacking: 'normal',
                    borderWidth: 0,
                    dataLabels: {
                        enabled: true,
                        color: '#000000',
                        formatter: function () {
                            return this.series.name == '已交房' ? this.point.y + "户<br/>" + parseFloat(this.point.y * 100 / this.point.stackTotal).toFixed(2) + '%' : "";
