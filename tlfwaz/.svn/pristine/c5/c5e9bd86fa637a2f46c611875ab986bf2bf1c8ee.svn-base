package com.app.cq.en;

/**
 * 特殊家庭状态
 * Created by jmdf on 2018/10/14.
 */
public enum SpecialStatus {

    YES("是", 1), NO("否", 2);

    // 成员变量
    private String name;
    private Integer index;

    // 构造方法
    private SpecialStatus(String name, Integer index) {
        this.name = name;
        this.index = index;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public Integer getIndex() {
        return index;
    }

    public void setIndex(Integer index) {
        this.index = index;
    }
}
