.file_category{
    width: 20%;
    float: left;
    background-color: #f5f5f5;
    min-height: 1000px;

}

.file_pic{
    width: 76%;
    float: left;
    margin-left: 30px;
}

.folder-list li a {
    color: #666666;
    display: block;
    padding: 5px 0;
    line-height: 26px;
}
.folder-list li {
    border-bottom: 1px solid #e7eaec;
    display: block;
}
.folder-list li i {
    margin-right: 8px;
    color: #3d4d5d;
}

.file-box {
    float: left;
    width: 220px;
}

.file {
    border: 1px solid #e7eaec;
    padding: 0;
    background-color: #ffffff;
    position: relative;
    margin-bottom: 20px;
    margin-right: 20px;
}

.file-manager h5 {
    text-transform: uppercase;
}
.file-manager {
    list-style: none outside none;
    margin: 0 0px 0 20px;
    padding: 0 20px 0 0;
}

.file-manager .hr-line-dashed {
    margin: 15px 0;
}

.file .image{
    margin: 4px;
}